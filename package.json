{
	"name": "vscode-paiza-io",
	"displayName": "vscode-paiza-io",
	"description": "paiza.IO for Visual Studio Code",
	"version": "0.1.0",
	"publisher": "mojamoja-cloud",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/s10akir/vscode-paiza-io"
	},
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-paiza-io.run"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-paiza-io.run",
				"title": "paizaIO: Run code on paiza.IO"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.59.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"@s10akir/node-paiza-io": "^1.0.1"
	}
}
